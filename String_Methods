Hands-on Samples
g = "hello world."

print(len(g))

print(g.index("world"))

print(g.capitalize())

print(g.upper())
print(g.lower())

print(g.islower())
print(g.isupper())

#print(g.find("world"))

#print(g.count("l"))

#print(g.split()) #'hello', 'world.'

print("show me\n d money")

print(g + "Hope u r doing ok")

print(g.replace("world", "people"))
